<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

	<!-- redirection des fichiers statiques js, css fnt et img -->

	<rule>
        <note>redirection des css</note>
        <from>^/(.*)/css/(.*)$</from>
        <to>/css/$2</to>
    </rule>
    
	<rule>
        <note>redirection des js</note>
        <from>^/(.*)/js/(.*)$</from>
        <to>/js/$2</to>
    </rule>
    
	<rule>
        <note>redirection des img</note>
        <from>^/(.*)/img/(.*)$</from>
        <to>/img/$2</to>
    </rule>
    
	<rule>
        <note>redirection des fnt</note>
        <from>^/(.*)/fnt/(.*)$</from>
        <to>/fnt/$2</to>
    </rule>
    
	<rule>
        <note>redirection des json</note>
        <from>^/(.*)/json/(.*)$</from>
        <to>/json/$2</to>
    </rule>
    
	<rule>
        <note>redirection des templates</note>
        <from>^/(.*)/tmpl/(.*)$</from>
        <to>/tmpl/$2</to>
    </rule>
    
	<rule>
        <note>redirection du boot</note>
        <from>^/(.*)/boot$</from>
        <to>/boot</to>
    </rule>
    
    <!-- redirection html -->

    <rule>
        <note>redirection vers la page d'ajout d'un bug github</note>
        <from>/bug-tracker</from>
        <to>/bug-tracker.html</to>
    </rule>
    
    <rule>
        <note>redirection vers la page du compte</note>
        <from>/comptes/nouveau</from>
        <to>/account.html</to>
    </rule>
    
    <rule>
        <note>redirection vers la page des listes de comptes</note>
        <from>/comptes</from>
        <to>/accounts.html</to>
    </rule>
    

</urlrewrite>
