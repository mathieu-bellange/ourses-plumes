<!--Partial Define-->
{{## def.article: art:
								<h4 class="{{= art.rubrique.classe}}">{{= art.rubrique.rubrique }}</h4>
								<div class="href-block clearfix {{= art.rubrique.classe}}">
									<h5 class="title">{{= art.title }}</h5>
									<p class="summary">
										{{? art.publishedDate !== null }}
											<span class="icon-date tiny" title="Date de publication"><span class="hide">Publi&eacute; le </span></span><time datetime="{{= getDateTime(new Date(art.publishedDate)) }}" class="date">{{= dateToString(new Date(art.publishedDate)) }}</time>
										{{?? art.updatedDate !== null}}
											<span class="icon-date tiny" title="Date de mise &agrave; jour"><span class="hide">Mis &agrave; jour le </span></span><time datetime="{{= getDateTime(new Date(art.updatedDate)) }}" class="date">{{= dateToString(new Date(art.updatedDate)) }}</time>
										{{?? art.createdDate !== null}}
											<span class="icon-date tiny" title="Date de cr&eacute;ation"><span class="hide">Cr&eacute;e le </span></span><time datetime="{{= getDateTime(new Date(art.createdDate)) }}" class="date">{{= dateToString(new Date(art.createdDate)) }}</time>
										{{?}}
										{{= art.description}}
										<span class="right author"><strong>{{= art.profile.pseudo }}</strong></span>
									</p>
									{{? art.status == "BROUILLON" }}
										{{? art.profile.id == window.localStorage.getItem($oursesProfileId)}}
											<a href="{{= art.path }}">Lire l&rsquo;article</a>
										{{??}}
											<a href="javascript:void(0)" style="cursor: default;"></a>
										{{?}}
									{{?? art.status == "AVERIFIER"}}
										{{? window.localStorage.getItem($oursesUserRole) == $role_admin}}
											<a href="{{= art.path }}">Lire l&rsquo;article</a>
										{{??}}
											<a href="javascript:void(0)" style="cursor: default;"></a>
										{{?}}
									{{??}}
										<a href="{{= art.path }}">Lire l&rsquo;article</a>
									{{?}}
									{{? art.status == "BROUILLON" }}
										<div class="validate" style="display: none;">
											<!--delete for Drafts-->
											<button class="small" data-delete="{{= art.id }}">Supprimer</button>
											{{? art.profile.id == window.localStorage.getItem($oursesProfileId)}}
												<button class="alert small" data-validate="{{= art.id }}">Valider</button>
											{{?}}
										</div>
									{{?? art.status == "AVERIFIER" }}
										<div class="validate" style="display: none;">
											<!--Validation for Standbys-->
											{{? window.localStorage.getItem($oursesUserRole) == $role_admin}}
												<button class="small success" data-publish="{{= art.id }}">Publier</button>
											{{?}}
											<button class="alert small" data-invalidate="{{= art.id }}">Invalider</button>
										</div>
									{{?? art.status == "ENLIGNE" }}
										{{? window.localStorage.getItem($oursesUserRole) == $role_admin || art.profile.id == window.localStorage.getItem($oursesProfileId)}}
											<div class="validate" style="display: none;">
												<!--Validation for Standbys-->
												<button class="small success" data-recall="{{= art.id }}">Rappeler</button>
											</div>
										{{?}}
									{{?}}
								</div>
#}}
				<!--Articles-->
				<section id="articles">
					{{? window.localStorage.getItem($oursesUserRole) == $role_admin || window.localStorage.getItem($oursesUserRole) == $role_redac }}
					<div class="clearfix">
						<div class="right">
							<a href="{{= $new_article}}" class="icon-article" title="R&eacute;diger un nouvel article" data-tooltip><span class="text">&Eacute;crire</span></a>
						</div>
					</div>
					{{?}}
					<ul id="articles_draft" class="no-bullet articles-list draft">
						{{~ data.drafts :value }}
							<li>{{# def.article:value }}</li>
						{{~}}
					</ul>
					<ul id="articles_standby" class="no-bullet articles-list standby">
						{{~ data.toCheck :value }}
							<li>{{# def.article:value }}</li>
						{{~}}
					</ul>
					<ul id="articles_publish" class="no-bullet articles-list">
						{{~ data.onLine :value }}
							<li>{{# def.article:value }}</li>
						{{~}}
					</ul>
				</section>