<!--Partial Define-->
{{## def.article: data:
	<h4 class="{{= data.rubrique.classe}}">{{= data.rubrique.rubrique}}</h4>
	<ul class="right inline-list hide-for-small">
		<li><span class="icon-twitter small">Twitter&#8239;:</span>&nbsp;7</li>
		<li><span class="icon-facebook small">Facebook&#8239;:</span>&nbsp;6</li>
		<li><span class="icon-googleplus small">GooglePlus&#8239;:</span>&nbsp;1</li>
	</ul>
	<div class="href-block clearfix cyan">
		<h5 class="title">{{= data.title}}</h5>
		<p class="summary">
			<time datetime="{{= new Date(data.publishedDate).toLocaleDateString()}}" class="left date"></time>
			{{= data.description}}
			<span class="right author"><strong>{{=data.profile.pseudo}}</strong></span>
		</p>
		<a href="{{= data.path }}" data-show="sexisme_dans_les_jeux_videos_une_critique_impossible">Lire l&rsquo;article</a>
	</div>
#}}

				<!--Articles-->
				<section id="articles">
					<!--
					<h2>Articles <small>.articles-list</small></h2>
					<h3>Tous les articles <small>.list</small></h3>
					-->
					<div class="clearfix">
						<dl class="sub-nav" style="display: inline-block;">
							<dt>Trier&#8239;:</dt>
							<dd class="active"><a href="javascript:void(0)" class="disabled">Date de publication</a></dd>
							<dd><a href="javascript:void(0)" class="disabled">Popularit&eacute;</a></dd>
							<dd><a href="javascript:void(0)" class="disabled">Rubrique</a></dd>
							<dd><a href="javascript:void(0)" class="disabled">Type</a></dd>
						</dl>
						{{? window.localStorage.getItem($oursesUserRole) == $role_admin || window.localStorage.getItem($oursesUserRole) == $role_redac  }}
							<a href="{{= $new_article}}" class="icon-article medium right" title="R&eacute;diger un article">R&eacute;diger un article</a>
						{{?}}
					</div>
					<ul class="no-bullet articles-list">
						{{~ data.drafts :value}}
							<li>{{# def.article:value}}</li>
						{{~}}						
					</ul>					
					<ul class="no-bullet articles-list">
						{{~ data.toCheck :value}}
							<li>{{# def.article:value}}</li>
						{{~}}						
					</ul>
					<ul class="no-bullet articles-list">
						{{~ data.onLine :value}}
							<li>{{# def.article:value}}</li>
						{{~}}						
					</ul>					
				</section>