<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

	<!--Redirections JS-->
	<rule>
		<note>redirection du boot</note>
		<from>^(.*)boot$</from>
		<to>/boot</to>
	</rule>

	<!--Redirections HTML-->
	<rule>
		<note>redirection vers la page d'ajout d'un bug github</note>
		<from>^/bug-tracker$</from>
		<to>/bug-tracker.html</to>
	</rule>

	<rule>
		<note>redirection vers la page du compte</note>
		<from>^/comptes/nouveau$</from>
		<to>/account.html</to>
	</rule>
	
	<rule>
		<note>redirection vers la page de modification de mon compte</note>
		<from>^/parametres/compte$</from>
		<to>/updateAccount.html</to>
	</rule>
	
	<rule>
		<note>redirection vers la page de modification de mon profil</note>
		<from>^/parametres/profil$</from>
		<to>/updateProfile.html</to>
	</rule>
	
	<rule>
		<note>redirection vers la page d'un profil</note>
		<from>^/profils/(.*)$</from>
		<to>/profile.html</to>
	</rule>

	<rule>
		<note>redirection vers la page des listes de comptes</note>
		<from>^/comptes$</from>
		<to>/accounts.html</to>
	</rule>
	
	<rule>
		<note>redirection vers la page de connexion</note>
		<from>^/connexion$</from>
		<to>/login.html</to>
	</rule>
	
	<rule>
		<note>redirection vers la page de cr√©ation d'un article</note>
		<from>^/articles/nouveau$</from>
		<to>/editor.html</to>
	</rule>
	
	<rule>
		<note>redirection vers la liste des articles</note>
		<from>^/articles$</from>
		<to>/articles.html</to>
	</rule>
	
	
	<rule>
		<note>redirection vers la page d'un article</note>
		<from>^/articles/(.*)/(.*)$</from>
		<to>/article.html</to>
	</rule>
	
	<rule>
		<note>redirection vers la page d'update d'un article</note>
		<from>^/articles/([0-9]+)$</from>
		<to>/editor.html</to>
	</rule>


</urlrewrite>
